{
    "Name": "java",
    "RuleSet": {
        "Rules": [
        {
            "Name": "Construct",
            "Type": 0,
            "InputSymbol": "construct",
            "OutputSymbol": "public class"
        },
        {
            "Name": "Label",
            "Type": 1,
            "Expression": "[a-zA-Z0-9_]+"
        },
        {
            "Name": "BlockStart",
            "Type": 0,
            "InputSymbol": "[",
            "OutputSymbol": "{"
        },
        {
            "Name": "BlockEnd",
            "Type": 0,
            "InputSymbol": "]",
            "OutputSymbol": "}"
        },
        {
            "Name": "BlockStartSeparator",
            "Type": 1,
            "Expression": "(' ' | '\n')*"
        },
        {
            "Name": "ConstructDeclarationBlockStart",
            "Type": 3,
            "Delimiter": " ",
            "Rules": ["Construct", "Label", "BlockStart", "BlockStartSeparator"]
        },
        {
            "Name": "Anything",
            "Type": 1,
            "Expression": ".+"
        },
        {
            "Name": "ConstructDeclaration",
            "Type": 2,
            "BlockStart": "ConstructDeclarationBlockStart",
            "BlockBody": "Anything",
            "BlockEnd": "BlockEnd"
        }
    ]
    }
}